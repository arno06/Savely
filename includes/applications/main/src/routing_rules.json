{
	"front":
	{
		"/":
		{
			"*":
			{
				"controller":"index",
				"action":"index"
			}
		},
		"list/{$permalink_list}":
		{
			"parameters":
			{
				"permalink_list":"[a-zA-Z0-9]{8}"
			},
			"GET":
			{
				"controller":"l",
				"action":"view"
			}
		},
		"list/{$permalink_list}/edit":
		{
			"parameters":
			{
				"permalink_list":"[a-zA-Z0-9]{8}",
				"edit":true
			},
			"GET":
			{
				"controller":"l",
				"action":"view"
			}
		},
		"{$controller}/":
		{
			"parameters":
			{
				"controller":"[a-zA-Z0-9\\-]+"
			},
			"*":
			{
				"action":"index"
			}
		},
		"{$controller}/{$action}/":
		{
			"parameters":
			{
				"controller":"[a-zA-z0-9\\-]+",
				"action":"[a-zA-z0-9\\-]+"
			},
			"*":
			{
				"controller":"{$controller}",
				"action":"{$action}"
			}
		}
	}
}